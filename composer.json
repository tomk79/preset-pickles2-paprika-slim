{
    "name": "tomk79/preset-pickles2-paprika-slim",
    "description": "preset - pickles2-paprika-slim",
    "keywords": ["pickles2"],
    "license": "MIT",
    "type": "project",
    "scripts":{
        "start": [
            "php -S 127.0.0.1:8088 -t ./dist/"
        ],
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit --colors"
        ]
    } ,
    "repositories": [],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "^6.4",
        "pickles2/px-fw-2.x": "^2.0",
        "pickles2/px2-dec": "^2.0",
        "pickles2/px2-multitheme": "^2.0",
        "pickles2/px2-paprika": "dev-develop",
        "pickles2/px2-path-resolver": "^2.0",
        "pickles2/px2-publish-ex": "^2.0",
        "pickles2/px2-px2dthelper": "^2.0",
        "pickles2/px2-remove-attr": "^2.0",
        "pickles2/px2-sitemapexcel": "^2.0",
        "tomk79/broccoli-module-interact": "^0.1",
        "tomk79/broccoli-module-lp": "^0.1",
        "tomk79/broccoli-module-medias": "^0.1",
        "tomk79/broccoli-module-std-document": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1.0"
    },
    "config": {
        "process-timeout": 0,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "files": []
    },
    "extra": {
        "px2package": {
            "name": "preset - pickles2-paprika-slim",
            "type": "project",
            "path": "src_px2/.px_execute.php",
            "path_homedir": "px-files/"
        }
    }
}
